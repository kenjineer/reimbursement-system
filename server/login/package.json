{
  "name": "login",
  "version": "1.0.0",
  "description": "A reimbursement system which has the functionality to accept or reject plans and applications for reimbursement for managers, file plans and applications for reimbursement for employees, and system administration for admins.",
  "main": "login.js",
  "scripts": {
    "start": "nodemon login.js",
    "redis": "redis-server",
    "startSonar": "StartSonar.bat",
    "sonarTest": "sonar-scanner.bat",
    "mochaTest": "mocha --exit",

    "dev-start": "npm-run-all -p redis start",
    "unit-test": "npm-run-all -p redis mochaTest",
    "coverage-test": "nyc --reporter=lcov --reporter=text npm run unit-test",
    "code-test": "npm-run-all -s startSonar sonarTest"
  },
  "author": "Kenneth Karamihan",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0"
  }
}
